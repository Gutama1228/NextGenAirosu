# Roblox AI Studio - Environment Variables Template
# Copy this file to .env and fill in your values

# Anthropic Claude API Configuration
REACT_APP_ANTHROPIC_API_KEY=sk-ant-api03-xxxxx

# Application Environment
REACT_APP_ENV=development

# API Base URL (Optional - for backend integration)
REACT_APP_API_URL=http://localhost:3001/api

# Feature Flags (Optional)
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG=false

# App Configuration (Optional)
REACT_APP_APP_NAME=Roblox AI Studio
REACT_APP_APP_VERSION=1.0.0

# Social Links (Optional)
REACT_APP_GITHUB_URL=https://github.com/yourusername/roblox-ai-studio
REACT_APP_TWITTER_URL=https://twitter.com/yourusername

# Notes:
# - NEVER commit .env file to git
# - REACT_APP_ prefix is required for Create React App
# - Restart dev server after changing .env variables
